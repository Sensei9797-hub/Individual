    <script>
        (function() {
            // –ü—Ä–∞–≤–∏–ª—å–Ω—ã–µ –æ—Ç–≤–µ—Ç—ã: false = –ù–µ–≤–µ—Ä–Ω–æ, true = –í–µ—Ä–Ω–æ
            const correctAnswers = [false, true, true, true, false, true, true];
            const questionNames = ['q1', 'q2', 'q3', 'q4', 'q5', 'q6', 'q7'];

            // –§—É–Ω–∫—Ü–∏—è —Å–±–æ—Ä–∞ –æ—Ç–≤–µ—Ç–æ–≤ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
            function getUserAnswers() {
                let userAnswers = [];
                for (let i = 0; i < questionNames.length; i++) {
                    const radios = document.getElementsByName(questionNames[i]);
                    let selectedValue = null;
                    for (let radio of radios) {
                        if (radio.checked) {
                            selectedValue = radio.value === 'true'; // true –µ—Å–ª–∏ "–í–µ—Ä–Ω–æ"
                            break;
                        }
                    }
                    userAnswers.push(selectedValue); // –º–æ–∂–µ—Ç –±—ã—Ç—å null, –µ—Å–ª–∏ –Ω–µ –≤—ã–±—Ä–∞–Ω–æ
                }
                return userAnswers;
            }

            // –§—É–Ω–∫—Ü–∏—è –ø–æ–¥—Å—á—ë—Ç–∞ –ø—Ä–∞–≤–∏–ª—å–Ω—ã—Ö
            function calculateScore(userAnswers) {
                let correctCount = 0;
                for (let i = 0; i < correctAnswers.length; i++) {
                    if (userAnswers[i] !== null && userAnswers[i] === correctAnswers[i]) {
                        correctCount++;
                    }
                }
                return correctCount;
            }

            // –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–Ω—è—Ç–∏—è –≤—ã–¥–µ–ª–µ–Ω–∏—è —Å–æ –≤—Å–µ—Ö –≤–æ–ø—Ä–æ—Å–æ–≤ (—É–±—Ä–∞—Ç—å –±–µ–π–¥–∂–∏/–ø–æ–¥—Å–≤–µ—Ç–∫—É)
            function resetVisualFeedback() {
                for (let i = 0; i < questionNames.length; i++) {
                    const qItem = document.getElementById(`q${i+1}`);
                    // –£–±—Ä–∞—Ç—å –≤—Å–µ –¥–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω—ã–µ –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä—ã, –µ—Å–ª–∏ –±—ã–ª–∏
                    const existingBadge = qItem.querySelector('.correct-badge');
                    if (existingBadge) existingBadge.remove();
                    qItem.style.backgroundColor = ''; // —Å–±—Ä–æ—Å —Ñ–æ–Ω–∞
                }
            }

            // –§—É–Ω–∫—Ü–∏—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ —Å –ø–æ–¥—Å–≤–µ—Ç–∫–æ–π
            function showResults() {
                const userAnswers = getUserAnswers();
                const score = calculateScore(userAnswers);
                const total = correctAnswers.length;

                // –û—á–∏—Å—Ç–∏—Ç—å –ø—Ä–µ–¥—ã–¥—É—â—É—é –ø–æ–¥—Å–≤–µ—Ç–∫—É
                resetVisualFeedback();

                // –ü–æ–¥—Å–≤–µ—Ç–∏—Ç—å –∫–∞–∂–¥—ã–π –≤–æ–ø—Ä–æ—Å
                for (let i = 0; i < questionNames.length; i++) {
                    const qItem = document.getElementById(`q${i+1}`);
                    const userAns = userAnswers[i];
                    const correct = correctAnswers[i];

                    // –°–æ–∑–¥–∞—ë–º –º–∞–ª–µ–Ω—å–∫–∏–π –±–µ–π–¥–∂
                    const badge = document.createElement('span');
                    badge.classList.add('correct-badge');

                    if (userAns === null) {
                        // –Ω–µ –æ—Ç–≤–µ—á–µ–Ω
                        badge.textContent = '‚ö†Ô∏è –Ω–µ—Ç –æ—Ç–≤–µ—Ç–∞';
                        badge.style.background = '#f2e0b0';
                        badge.style.color = '#7a5d00';
                        qItem.style.backgroundColor = '#fff6e5';
                    } else if (userAns === correct) {
                        badge.textContent = '‚úì –ø—Ä–∞–≤–∏–ª—å–Ω–æ';
                        badge.classList.add('true');
                        qItem.style.backgroundColor = '#f0fff2';
                    } else {
                        badge.textContent = '‚úó –Ω–µ–≤–µ—Ä–Ω–æ';
                        badge.classList.add('false');
                        qItem.style.backgroundColor = '#ffeeed';
                    }

                    // –£–¥–∞–ª–∏—Ç—å —Å—Ç–∞—Ä—ã–π –±–µ–π–¥–∂, –µ—Å–ª–∏ –µ—Å—Ç—å, –∏ –¥–æ–±–∞–≤–∏—Ç—å –Ω–æ–≤—ã–π
                    const oldBadge = qItem.querySelector('.correct-badge');
                    if (oldBadge) oldBadge.remove();
                    qItem.appendChild(badge);
                }

                // –û—Ç–æ–±—Ä–∞–∑–∏—Ç—å –æ–±—â–∏–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç
                const resultMsg = document.getElementById('resultMessage');
                const scoreDisplay = document.getElementById('scoreDisplay');
                if (score === total) {
                    resultMsg.textContent = '–ü—Ä–µ–≤–æ—Å—Ö–æ–¥–Ω–æ! –í—Å–µ –æ—Ç–≤–µ—Ç—ã –≤–µ—Ä–Ω—ã.';
                } else if (score >= 5) {
                    resultMsg.textContent = '–•–æ—Ä–æ—à–∏–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç! –ù—É–∂–Ω–æ –ø–æ–¥—Ç—è–Ω—É—Ç—å —Ç–µ–æ—Ä–∏—é.';
                } else {
                    resultMsg.textContent = '–°—Ç–æ–∏—Ç –ø–æ–≤—Ç–æ—Ä–∏—Ç—å —Ñ–æ—Ä–º—É–ª—ã –ø–æ–ª–æ–≤–∏–Ω–Ω–æ–≥–æ —É–≥–ª–∞.';
                }
                scoreDisplay.textContent = `${score} –∏–∑ ${total}`;
            }

            // –ü–æ–ª–Ω—ã–π —Å–±—Ä–æ—Å (–æ—á–∏—Å—Ç–∏—Ç—å –≤—ã–±–æ—Ä, —É–±—Ä–∞—Ç—å –ø–æ–¥—Å–≤–µ—Ç–∫—É)
            function resetTest() {
                // —Å–Ω—è—Ç—å –≤—Å–µ —Ä–∞–¥–∏–æ–∫–Ω–æ–ø–∫–∏
                for (let i = 0; i < questionNames.length; i++) {
                    const radios = document.getElementsByName(questionNames[i]);
                    for (let radio of radios) {
                        radio.checked = false;
                    }
                }
                // —É–±—Ä–∞—Ç—å –≤–∏–∑—É–∞–ª—å–Ω—ã–µ –ø–æ–¥—Å–∫–∞–∑–∫–∏
                resetVisualFeedback();
                // —Å–±—Ä–æ—Å–∏—Ç—å —Ç–µ–∫—Å—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞
                document.getElementById('resultMessage').textContent = '–ñ–¥—É –≤–∞—à–∏—Ö –æ—Ç–≤–µ—Ç–æ–≤!';
                document.getElementById('scoreDisplay').textContent = '‚Äî';
                // —É–±—Ä–∞—Ç—å —Ñ–æ–Ω —É –≤–æ–ø—Ä–æ—Å–æ–≤ (–¥–æ–ø. –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç—å)
                for (let i = 0; i < questionNames.length; i++) {
                    const qItem = document.getElementById(`q${i+1}`);
                    qItem.style.backgroundColor = '';
                }
            }

            // –û–±—Ä–∞–±–æ—Ç—á–∏–∫–∏ –∫–Ω–æ–ø–æ–∫
            document.getElementById('checkBtn').addEventListener('click', function() {
                showResults();
            });

            document.getElementById('resetBtn').addEventListener('click', function() {
                resetTest();
            });

            // –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–æ: –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –∑–∞—â–∏—Ç—É –æ—Ç —Å–ª—É—á–∞–π–Ω–æ–π –æ—Ç–ø—Ä–∞–≤–∫–∏ –±–µ–∑ –æ—Ç–≤–µ—Ç–æ–≤, –Ω–æ –ø–æ–∫–∞–∑—ã–≤–∞—Ç—å –∫–∞–∫ –µ—Å—Ç—å.
            // –ü—Ä–∏ –∂–µ–ª–∞–Ω–∏–∏ –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ, –Ω–æ –Ω–µ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ.

            // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è: —Å–±—Ä–æ—Å –ø—Ä–∏ –∑–∞–≥—Ä—É–∑–∫–µ —Å—Ç—Ä–∞–Ω–∏—Ü—ã (—á–∏—Å—Ç–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ)
            window.addEventListener('load', function() {
                resetTest();
            });
        })();
    </script>

    <!-- –î–ª—è –∫—Ä–∞—Å–∏–≤–æ–≥–æ –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è —Ñ–æ—Ä–º—É–ª –¥–æ–±–∞–≤–∏–º MathJax (–Ω–µ –æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ, –Ω–æ —Å–¥–µ–ª–∞–µ—Ç –∑–∞–ø–∏—Å—å –ø—Ä–∏—è—Ç–Ω–µ–µ) -->
    <script src="https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml.js" id="MathJax-script" async></script>
    <!-- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ MathJax –¥–ª—è –∏–Ω–ª–∞–π–Ω —Ñ–æ—Ä–º—É–ª -->
    <script>
        window.MathJax = {
            tex: {
                inlineMath: [['\\(', '\\)']]
            }
        };
    </script>
